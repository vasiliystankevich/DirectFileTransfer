@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<ServerSTUNLibrary.Models.FileUriDataModel>

<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title>Index - List download files</title>
</head>
    <body>
        <h1>List download files</h1>
        @foreach (var fileData in @Model.Files)
        {
            <a href='@fileData.Uri'>@fileData.FileName</a><br/>
        }
    </body>
</html>